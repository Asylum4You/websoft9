[Unit]
Description=mingdao service
After=network.target
[Service]
Type=simple
ExecStart=/bin/bash /usr/local/MDPrivateDeployment/service.sh start
ExecStop=-/bin/bash /usr/local/MDPrivateDeployment/service.sh stopall
ExecStopPost=-/usr/bin/pkill mingdaoyun 
PIDFile=/data/apps/mingdao/installer/service.pid
[Install]
WantedBy=multi-user.target
