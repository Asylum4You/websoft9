{
  "requires": {
    "cockpit": "239"
  },
  "locales": {
    "en-us": "English",
    "zh-cn": "中文（中国）"
  },
  "bridges": [
    {
      "privileged": true,
      "environ": [
        "SUDO_ASKPASS=${libexecdir}/cockpit-askpass"
      ],
      "spawn": [
        "sudo",
        "-k",
        "-A",
        "cockpit-bridge",
        "--privileged"
      ]
    },
    {
      "privileged": true,
      "spawn": [
        "pkexec",
        "--disable-internal-agent",
        "cockpit-bridge",
        "--privileged"
      ]
    }
  ]
}
